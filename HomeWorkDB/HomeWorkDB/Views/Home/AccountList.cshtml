@model  IEnumerable<HomeWorkDB.ViewModels.AccountViewModel>
@{
    Layout = null;
}

<div class="row">
    <div class="col-md-12">
        <table class="table table-bordered table-hover">
            <tbody>

                <tr>
                    <th>#</th>
                    <th>類別</th>
                    <th>日期</th>
                    <th>金額</th>
                </tr>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Categoryyy</td>
                        <td>@item.Dateee</td>
                        <td>@item.Amounttt</td>
                    </tr>           
                }
            </tbody>
        </table>
    </div>
</div>
